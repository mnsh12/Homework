package solution;

import java.util.Scanner;

public class Q8 {
	public static fact(int n) {
		int sum  = 1;
		for (int i =0;i<=n;i++) {
			sum*=i;
			
		}
		return sum;
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int n = sc.nextInt();
		
		
	}
}
