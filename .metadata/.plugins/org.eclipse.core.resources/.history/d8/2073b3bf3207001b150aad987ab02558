package solution;

import java.util.Scanner;

public class Q7 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String n = sc.next();
		switch(n) {
		case "i":
			for(int i = 0; i< 5;i++) {
				System.out.println("*********");
			}
		case "ii":
			for(int i = 0; i< 5;i++) {
				for(int j = 0; j< i;j++) {
					System.out.print("*");
				}
				System.out.println();
			}
		}
	}

}
